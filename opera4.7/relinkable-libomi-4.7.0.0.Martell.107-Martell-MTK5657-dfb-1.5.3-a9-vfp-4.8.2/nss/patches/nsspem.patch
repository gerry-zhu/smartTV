diff -urN nss-3.19.1/nss/lib/ckfw/pem/ckpem.h nss-3.19.1-nsspem/nss/lib/ckfw/pem/ckpem.h
--- nss-3.19.1/nss/lib/ckfw/pem/ckpem.h	1970-01-01 01:00:00.000000000 +0100
